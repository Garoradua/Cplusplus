#include <iostream>
using namespace std;
int main() {
	cout << "Enter total no of test cases" << endl;
	int n;
	cin >> n ;
	int x=1;
	int count=0;
	for(x=1;x<=n;x++)
	{
		cout << "Enter two numbers" << endl;
		int a,b;
		cin >> a >> b ;
		for(int i=a;i<=b;i++)
		{
			if(i%2==0)
			{
				count++;
			}
		}
	}
	return 0;
}